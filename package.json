    "dev:watch": "tsc --watch",
    "build": "tsc",
    "test": "jest"
  },
  "keywords": [
    "nodejs",
    "restful",
    "api",
    "server"
  ],
  "author": "Iqbal Hossen",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.12",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "dependencies": {
    "multer": "^2.0.1"
  }
