{
  "name": "nodejs-custom-server",
  "version": "1.0.0",
  "description": "This is a custom Node.js server that provides a RESTful API server.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "nodemon --watch src --exec node --loader ts-node/esm src/index.ts",
    "lint": "eslint 'src/**/*.ts' --fix",
